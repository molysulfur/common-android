<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <variable
            name="viewModel"
            type="com.awonar.app.ui.profile.chart.ProfileChartViewModel" />

        <variable
            name="userViewModel"
            type="com.awonar.app.ui.user.UserViewModel" />
    </data>

    <androidx.recyclerview.widget.RecyclerView xmlns:android="http://schemas.android.com/apk/res/android"
        setProfileChart="@{viewModel.positionChartItems}"
        userViewModel="@{userViewModel}"
        viewModel="@{viewModel}"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/awonar_color_surface" />

</layout>
