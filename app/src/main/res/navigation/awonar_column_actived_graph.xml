<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/awonar_column_actived_graph"
    app:startDestination="@id/portFolioColumnActivedFragment">

    <fragment
        android:id="@+id/portFolioColumnActivedFragment"
        android:name="com.awonar.app.ui.portfolio.PortFolioColumnActivedFragment"
        android:label="PortFolioColumnActivedFragment"
        tools:layout="@layout/awonar_fragment_portfolio_column_actived">
        <action
            android:id="@+id/action_portFolioColumnActivedFragment_to_portFolioColumnFragment"
            app:destination="@id/portFolioColumnFragment"
            app:enterAnim="@anim/common_anim_right_to_left_in"
            app:exitAnim="@anim/common_anim_right_to_left_out"
            app:popEnterAnim="@anim/common_anim_left_to_right_in"
            app:popExitAnim="@anim/common_anim_left_to_right_out" />
    </fragment>

    <fragment
        android:id="@+id/portFolioColumnFragment"
        android:name="com.awonar.app.ui.portfolio.PortFolioColumnFragment"
        android:label="PortFolioColumnFragment"
        tools:layout="@layout/awonar_fragment_portfolio_column_list">
        <argument
            android:name="actived_column_name"
            app:argType="string" />
    </fragment>
</navigation>