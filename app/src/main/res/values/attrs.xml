<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NumberPickerCollapsibleView">
        <attr name="numberPickerCollapsibleView_setTitle" format="string" />
        <attr name="numberPickerCollapsibleView_setDescription" format="string" />
        <attr name="numberPickerCollapsibleView_setNumber" format="float" />
        <attr name="numberPickerCollapsibleView_setDigit" format="integer" />
    </declare-styleable>

    <declare-styleable name="NumberPickerEditText">
        <attr name="numberPickerEditText_setPrefix" format="string" />
        <attr name="numberPickerEditText_setNumber" format="float" />
        <attr name="numberPickerEditText_setHelper" format="string" />
        <attr name="numberPickerEditText_setPlaceHolder" format="string" />
        <attr name="numberPickerEditText_setEnablePlaceHolder" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TextDividerView">
        <attr name="textDividerView_setText" format="string" />
    </declare-styleable>

    <declare-styleable name="VerticalTitleView">
        <attr name="verticalTitleView_setTitle" format="string" />
        <attr name="verticalTitleView_setSubTitle" format="string" />
        <attr name="verticalTitleView_titleTextColor" format="color" />
        <attr name="verticalTitleView_subTitleTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ImageCheckBoxView">
        <attr name="imageCheckBoxView_setText" format="string" />
        <attr name="imageCheckBoxView_setIcon" format="reference" />
        <attr name="imageCheckBoxView_isCheck" format="boolean" />
    </declare-styleable>

    <declare-styleable name="InstrumentCardView">
        <attr name="instrumentCardView_setImage" format="reference" />
        <attr name="instrumentCardView_setTitle" format="string" />
        <attr name="instrumentCardView_setSubTitle" format="string" />
        <attr name="instrumentCardView_setPrice" format="float" />
        <attr name="instrumentCardView_setChange" format="float" />
        <attr name="instrumentCardView_setPercentChange" format="float" />
    </declare-styleable>
</resources>